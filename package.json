{
  "name": "positive-vibes",
  "version": "1.0.0",
  "description": "A minimal Electron application written with Typescript",
  "scripts": {
    "clean": "rimraf ./dist ./node_modules/ && npm install",
    "build": "tsc",
    "watch": "nodemon -e ts,html,css -x \"npm run start\"",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "npm run build && electron ./dist/main.js"
  },
  "repository": "https://github.com/ScrewTSW/PositiveVibes",
  "keywords": [
    "Electron",
    "buttplug",
    "typescript"
  ],
  "author": "ScrewTSW",
  "license": "WTFPL",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.0.0",
    "electron": "^27.0.0",
    "eslint": "^8.54.0",
    "typescript": "^5.2.2",
    "buttplug": "3.2.1",
    "rimraf": "^5.0.5",
    "nodemon": "3.0.1"
  },
  "dependencies": {}
}
